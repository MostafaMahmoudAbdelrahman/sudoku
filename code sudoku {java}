                    x6 = 0;
                    x5 = 0;
                }
                x6 = 0;
                last_second.setText("" + x6);
                x5++;
                first_second.setText("" + x5);
            } else {
                x6++;
                last_second.setText("" + x6);
            }
        }
    }

